{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "./src/components/shared/*"
      ],
      "@home/*": [
        "./src/components/home/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@images/*": [
        "./src/assets/images/*"
      ],
      "@blog/*": [
        "./src/components/blog/*"
      ],
      "@styles/": [
        "./src/styles/*"
      ],
      "@stylePages/": [
        "./src/styles/pages/*"
      ],
    }
  }
}