{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "./src/components/shared/*"
      ],
      "@static/*": [
        "./src/components/static/*"
      ],
      "@home/*": [
        "./src/components/static/home/*"
      ],
      "@about/*": [
        "./src/components/static/about/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@images/*": [
        "./src/assets/images/*"
      ],
      "@essay/*": [
        "./src/components/essay/*"
      ],
      "@lib/*": [
        "./src/lib/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@imageAssets/*": [
        "./src/assets/images/*"
      ],
      "@/*": [
        "./src/*"
      ],
    }
  }
}