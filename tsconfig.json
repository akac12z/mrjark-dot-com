{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@shared/*": [
        "./src/components/shared/*"
      ],
      "@home/*": [
        "./src/components/static/home/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@about/*": [
        "./src/components/static/about/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@images/*": [
        "./src/assets/images/*"
      ],
      "@essay/*": [
        "./src/components/essay/*"
      ],
      "@styles/": [
        "./src/styles/*"
      ],
      "@stylePages/": [
        "./src/styles/pages/*"
      ],
    }
  }
}