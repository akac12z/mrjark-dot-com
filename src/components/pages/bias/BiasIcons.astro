---
import type { CollectionEntry } from "astro:content";
import AnchorEffect from "./icons/AnchorEffect.icon.astro";
import BackfireEffect from "./icons/BackfireEffect.icon.astro";

interface Props {
  entry: CollectionEntry<"bias">;
}

const { entry } = Astro.props;
const { data } = entry;
const { biasID } = data;
---

<div>
  {biasID === "efecto-ancla" && <AnchorEffect />}
  {biasID === "efecto-backfire" && <BackfireEffect />}
</div>

